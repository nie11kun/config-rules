{
    "inbounds": [
        {
            "port": 1080,
            "listen": "0.0.0.0",
            "tag": "socks-inbound",
            "protocol": "socks",
            "settings": {
                "auth": "noauth",
                "udp": false,
                "ip": "127.0.0.1"
            },
            "sniffing": {
                "enable": true,
                "destOverride": [
                    "http",
                    "tls"
                ]
            },
            "allocate": {
                "strategy": "always",
                "refresh": 5,
                "concurrency": 3
            }
        },
        {
            "port": 1082,
            "listen": "0.0.0.0",
            "tag": "http-inbound",
            "protocol": "http",
            "settings": {
                "timeout": 0
            },
            "sniffing": {
                "enable": true,
                "destOverride": [
                    "http",
                    "tls"
                ]
            },
            "allocate": {
                "strategy": "always",
                "refresh": 5,
                "concurrency": 3
            }
        }
    ]
}
