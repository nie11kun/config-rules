{
    "routing": {
        "domainStrategy": "IPIfNonMatch",
        "domainMatcher": "mph",
        "rules": [
            {
                "type": "field",
                "inboundTag": [
                    "http-inbound",
                    "socks-inbound"
                ],
                "port": 53,
                "outboundTag": "dns-out"
            },
            {
                "type": "field",
                "domain": [
                    "geosite:category-ads-all",
                    "360.com",
                    "360.cn"
                ],
                "outboundTag": "blocked"
            },
            {
                "type": "field",
                "domain": [
                    "geosite:cn",
                    "geosite:speedtest",
                    "geosite:apple-cn",
                    "geosite:google-cn",
                    "geosite:category-games@cn",
                    "niekun.net"
                ],
                "outboundTag": "direct-customDNS"
            },
            {
                "type": "field",
                "ip": [
                    "geoip:cn",
                    "114.114.114.114",
                    "223.5.5.5",
                    "119.29.29.29"
                ],
                "outboundTag": "direct-customDNS"
            },
            {
                "type": "field",
                "ip": [
                    "geoip:private",
                    "27.168.0.0/16"
                ],
                "outboundTag": "direct-systemDNS"
            },
            {
                "type": "field",
                "protocol":["bittorrent"], 
                "outboundTag": "direct-systemDNS"
            },
            {
                "type": "field",
                "domain": [
                    "geosite:geolocation-!cn",
                    "testflight.apple.com"
                ],
                "balancerTag": "out-balancer"
            },
            {
                "type": "field",
                "ip": [
                    "geoip:telegram",
                    "geoip:google",
                    "geoip:twitter",
                    "1.1.1.1",
                    "8.8.8.8"
                ],
                "balancerTag": "out-balancer"
            }
        ],
        "balancers": [
            {
                "tag": "out-balancer",
                "selector": [
                    "out"
                ],
                "strategy": {
                    "type": "leastPing"
                }
            }
        ]
    }
}
