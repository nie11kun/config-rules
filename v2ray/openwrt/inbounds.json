{
    "inbounds": [
        {
            "port": 1081,
            "listen": "0.0.0.0",
            "tag": "dokodemo-door-inbound",
            "protocol": "dokodemo-door",
            "settings": {
                "network": "tcp,udp",
                "timeout": 30,
                "followRedirect": true
            },
            "sniffing": {
                "enable": true,
                "destOverride": [
                    "http",
                    "tls"
                ]
            },
            "allocate": {
                "strategy": "always",
                "refresh": 5,
                "concurrency": 3
            }
        }
    ]
}
