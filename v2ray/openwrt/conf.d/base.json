{
    "log": {
        "access": "/etc/v2ray/log/v2ray_access.log",
        "error": "/etc/v2ray/log//v2ray_error.log",
        "loglevel": "error"
    },
    "policy": {
        "0": {
            "handshake": 4,
            "connIdle": 300,
            "uplinkOnly": 2,
            "downlinkOnly": 5,
            "statsUserUplink": false,
            "statsUserDownlink": false,
            "bufferSize": 10240
        }
    },
    "system": {
        "statsInboundUplink": false,
        "statsInboundDownlink": false,
        "statsOutboundUplink": false,
        "statsOutboundDownlink": false
    }
}